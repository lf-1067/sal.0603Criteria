<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.TblJdEntity" table="tbl_jd" schema="sal">
        <id name="jdid">
            <column name="jdid" sql-type="int(11)"/>
        </id>
        <property name="jd">
            <column name="jd" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>